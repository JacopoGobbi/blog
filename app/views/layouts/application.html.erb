<!DOCTYPE html>
<html>
<head>
  <title>BloggingService</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag "https://netdna.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css", type: "text/css" %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% flash.each do |message_type, message| %>
  <%= message %>
<% end %>

<%= yield %>

</body>
</html>
